{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "mochi",
  "title": "mochi",
  "description": "",
  "icon": "command-icon.png",
  "author": "miralsuthar",
  "categories": [
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "ticker",
      "title": "ticker",
      "description": "Template for showing a static list and a detail view",
      "mode": "view"
    },
    {
      "name": "watchlist",
      "title": "watchlist",
      "description": "Template for showing a static list and a detail view",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.3",
    "@raycast/utils": "^1.5.2",
    "axios": "^1.4.0",
    "canvas": "^2.11.2",
    "chart.js": "^2.8.0",
    "node-fetch": "^3.3.1",
    "sparkline-svg": "^1.0.4",
    "use-debounce": "^9.0.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/chart.js": "^2.9.37",
    "@types/get-image-colors": "^4.0.2",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "preferences": [
    {
      "name": "DiscordId",
      "title": "Discord Id",
      "description": "Your Discord Id Example: Discord#1234",
      "type": "textfield",
      "default": "",
      "required": true,
      "placeholder": "Discord#1234"
    }
  ],
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
